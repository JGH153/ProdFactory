		"next": "^16",
		"radix-ui": "^1.4.3",
		"react": "^19",
		"react-dom": "^19",
		"tailwind-merge": "^3.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "latest",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^22",
		"@types/react": "^19",
